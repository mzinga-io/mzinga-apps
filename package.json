{
  "name": "mzinga-apps",
  "version": "0.34.7",
  "main": "dist/server.js",
  "repository": {
    "type": "git",
    "url": "https://newesis@dev.azure.com/newesis/Code%20Name%20MZinga/_git/mzinga-apps"
  },
  "author": "Newesis Srl",
  "license": "MIT",
  "private": true,
  "scripts": {
    "version:show": "cat package.json | grep version",
    "clean": "npm dedupe && npm prune",
    "dev": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts nodemon",
    "build:payload": "npm run payload:base build",
    "build:server": "tsc",
    "build": "npm run copyfiles && npm run build:payload && npm run build:server",
    "serve": "cross-env PAYLOAD_CONFIG_PATH=dist/payload.config.js NODE_ENV=production node --require './dist/tracing.js' dist/server.js",
    "copyfiles": "copyfiles -u 1 \"src/**/*.{html,css,scss,ttf,woff,woff2,eot,svg,jpg,png}\" dist/",
    "generate:types": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload generate:types",
    "generate:graphQLSchema": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload generate:graphQLSchema",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "jest __tests__/unit-tests --verbose",
    "test:integration": "jest __tests__/integration-tests --verbose --forceExit",
    "test:watch": "jest --watch",
    "coverage": "jest --ci --reporters=jest-junit --reporters=default --coverage --coverageReporters=cobertura --verbose",
    "lint": "eslint . --ext .ts",
    "migrateVersions": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts npx ts-node -T migrateVersions.ts",
    "format": "prettier --write src/*.{ts,tsx,css} src/**/*.{ts,tsx,css} src/*.{ts,tsx,css,html}",
    "payload:base": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts cross-env IS_BUILD_PROCESS=1 payload"
  },
  "engines": {
    "node": ">=14",
    "yarn": ">=1.22 <2"
  },
  "dependencies": {
    "@aengz/payload-redis-cache": "^1.1.1",
    "@json2csv/plainjs": "^7.0.4",
    "@monaco-editor/react": "^4.6.0",
    "@newesissrl/nodemailer-sendgrid": "^1.0.0",
    "@newesissrl/payload-exportcollections-plugin": "0.1.9",
    "@newesissrl/payload-zitadel-plugin": "0.0.38",
    "@opentelemetry/api": "1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.62.0",
    "@opentelemetry/core": "2.0.1",
    "@opentelemetry/exporter-prometheus": "^0.203.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.203.0",
    "@opentelemetry/exporter-trace-otlp-proto": "^0.203.0",
    "@opentelemetry/instrumentation": "^0.203.0",
    "@opentelemetry/resource-detector-container": "^0.7.0",
    "@opentelemetry/resources": "2.0.1",
    "@opentelemetry/sdk-node": "^0.203.0",
    "@opentelemetry/sdk-trace-base": "2.0.1",
    "@opentelemetry/sdk-trace-node": "2.0.1",
    "@opentelemetry/semantic-conventions": "1.36.0",
    "@payloadcms/bundler-webpack": "1.0.7",
    "@payloadcms/db-mongodb": "^1.0.2",
    "@payloadcms/db-postgres": "^0.8.4",
    "@payloadcms/plugin-form-builder": "^1.0.15",
    "@payloadcms/plugin-seo": "^3.53.0",
    "@payloadcms/richtext-slate": "^1.0.1",
    "@sendgrid/mail": "^8.0.0",
    "assert": "^2.0.0",
    "axios": "^1.7.7",
    "body-parser": "^2.2.0",
    "cookie-parser": "^1.4.6",
    "copyfiles": "^2.4.1",
    "cross-env": "^10.0.0",
    "dotenv": "^16.4.5",
    "escape-html": "^1.0.3",
    "express": "^4.21.1",
    "helmet": "^8.0.0",
    "jsoneditor-react": "^3.1.2",
    "monaco-editor": "^0.52.2",
    "node-polyfill-webpack-plugin": "^4.0.0",
    "nodemailer": "^7.0.0",
    "payload": "2.32.3",
    "payload-swagger": "^1.4.0",
    "rabbitmq-client": "^5.0.0",
    "react-jss": "^10.10.0",
    "redis": "^5.8.1",
    "sass": "^1.90.0",
    "stream-browserify": "^3.0.0",
    "tty-browserify": "^0.0.1",
    "typescript": "5.9.2",
    "util": "^0.12.5",
    "uuid": "^11.0.0",
    "pbkdf2": ">=3.1.3"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "^24.2.1",
    "@types/webpack": "^5.28.5",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "assert": "^2.1.0",
    "buffer": "^6.0.3",
    "eslint": "^9.33.0",
    "husky": "^9.0.0",
    "jest": "29.7.0",
    "jest-junit": "^16.0.0",
    "nodemon": "^3.0.0",
    "querystring-es3": "^0.2.1",
    "ts-jest": "29.4.0",
    "ts-node": "^10.0.0"
  },
  "resolutions": {
    "@types/mime": "2.0.3",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  }
}