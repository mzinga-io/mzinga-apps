{
  "name": "mzinga-apps",
  "version": "0.34.7",
  "main": "dist/server.js",
  "repository": {
    "type": "git",
    "url": "https://newesis@dev.azure.com/newesis/Code%20Name%20MZinga/_git/mzinga-apps"
  },
  "author": "Newesis Srl",
  "license": "MIT",
  "private": true,
  "scripts": {
    "version:show": "cat package.json | grep version",
    "clean": "npm dedupe && npm prune",
    "dev": "cross-env PAYLOAD_CONFIG_PATH=src/mzinga.config.ts nodemon",
    "build:mzinga": "npm run mzinga:base build",
    "build:server": "tsc",
    "build": "npm run copyfiles && npm run build:mzinga && npm run build:server",
    "serve": "cross-env PAYLOAD_CONFIG_PATH=dist/mzinga.config.js NODE_ENV=production node --require './dist/tracing.js' dist/server.js",
    "copyfiles": "copyfiles -u 1 \"src/**/*.{html,css,scss,ttf,woff,woff2,eot,svg,jpg,png}\" dist/",
    "generate:types": "cross-env PAYLOAD_CONFIG_PATH=src/mzinga.config.ts mzinga generate:types",
    "generate:graphQLSchema": "cross-env PAYLOAD_CONFIG_PATH=src/mzinga.config.ts mzinga generate:graphQLSchema",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "jest __tests__/unit-tests --verbose",
    "test:integration": "jest __tests__/integration-tests --verbose --forceExit",
    "test:watch": "jest --watch",
    "coverage": "jest --ci --reporters=jest-junit --reporters=default --coverage --coverageReporters=cobertura --verbose",
    "lint": "eslint . --ext .ts",
    "migrateVersions": "cross-env PAYLOAD_CONFIG_PATH=src/mzinga.config.ts npx ts-node -T migrateVersions.ts",
    "format": "prettier --write src/*.{ts,tsx,css} src/**/*.{ts,tsx,css} src/*.{ts,tsx,css,html}",
    "mzinga:base": "cross-env PAYLOAD_CONFIG_PATH=src/mzinga.config.ts cross-env IS_BUILD_PROCESS=1 mzinga"
  },
  "engines": {
    "node": ">=18",
    "yarn": ">=1.22 <2"
  },
  "dependencies": {
    "@aengz/payload-redis-cache": "^1.1.1",
    "@json2csv/plainjs": "^7.0.4",
    "@monaco-editor/react": "^4.5.1",
    "@mzinga/bundler-webpack": "0.1.3",
    "@mzinga/db-mongodb": "0.1.3",
    "@mzinga/db-postgres": "0.1.3",
    "@mzinga/plugin-form-builder": "0.1.3",
    "@mzinga/plugin-seo": "0.1.3",
    "@mzinga/richtext-slate": "0.1.3",
    "@newesissrl/nodemailer-sendgrid": "^1.0.0",
    "@newesissrl/payload-exportcollections-plugin": "1.0.3",
    "@newesissrl/payload-zitadel-plugin": "1.0.3",
    "@opentelemetry/api": "1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.66.0",
    "@opentelemetry/core": "2.2.0",
    "@opentelemetry/exporter-prometheus": "^0.207.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.207.0",
    "@opentelemetry/exporter-trace-otlp-proto": "^0.206.0",
    "@opentelemetry/instrumentation": "^0.207.0",
    "@opentelemetry/resource-detector-container": "^0.7.0",
    "@opentelemetry/resources": "2.2.0",
    "@opentelemetry/sdk-node": "^0.205.0",
    "@opentelemetry/sdk-trace-base": "2.1.0",
    "@opentelemetry/sdk-trace-node": "2.2.0",
    "@opentelemetry/semantic-conventions": "1.37.0",
    "@sendgrid/mail": "^8.0.0",
    "assert": "^2.0.0",
    "axios": "^1.12.2",
    "body-parser": "^2.2.0",
    "cookie-parser": "^1.4.6",
    "copyfiles": "^2.4.1",
    "cross-env": "^10.1.0",
    "dotenv": "^17.2.2",
    "escape-html": "^1.0.3",
    "express": "^4.21.1",
    "glob": "11.0.3",
    "helmet": "^8.0.0",
    "jsoneditor-react": "^3.1.2",
    "monaco-editor": "^0.52.0",
    "@monaco-editor/loader": "1.5.0",
    "mzinga": "0.1.3",
    "node-polyfill-webpack-plugin": "^4.0.0",
    "nodemailer": "^7.0.9",
    "pbkdf2": ">=3.1.5",
    "rabbitmq-client": "^5.0.5",
    "react-jss": "^10.10.0",
    "redis": "^5.8.3",
    "sass": "^1.90.0",
    "stream-browserify": "^3.0.0",
    "tty-browserify": "^0.0.1",
    "typescript": "5.9.3",
    "util": "^0.12.5"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "^24.9.1",
    "@types/webpack": "^5.28.5",
    "@typescript-eslint/eslint-plugin": "^8.46.0",
    "@typescript-eslint/parser": "^8.46.2",
    "assert": "^2.1.0",
    "buffer": "^6.0.3",
    "eslint": "^9.37.0",
    "husky": "^9.0.0",
    "jest": "30.2.0",
    "jest-junit": "^16.0.0",
    "nodemon": "^3.0.0",
    "querystring-es3": "^0.2.1",
    "ts-jest": "29.4.5",
    "ts-node": "^10.0.0"
  },
  "resolutions": {
    "@types/mime": "2.0.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "nodemailer": "^7.0.9"
  }
}